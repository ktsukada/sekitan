<%= simple_form_for(@company) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :tel_no %>
    <%= f.input :start_at %>
    <%= f.input :end_at %>
  <div class="form-inputs">
    <%= f.button :submit ,:disable_with => '送信中...'%>
  </div>
<% end %>
